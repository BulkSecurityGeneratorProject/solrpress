!!! 5
html(lang='en', data-ng-app='kewl')
  head
    title Kewl brunch skeleton
    link(rel='stylesheet', href='css/app.css')
    link(rel='icon', type='image/x-icon', href='favicon.ico')
    meta(charset='utf-8')
  body
    .navbar.navbar-default
      .container
        a.navbar-brand(href='.') Angular
          i.icon.icon-heart.icon-rotate-90.icon-fw
          |  APIs =
          strong  Kewl
        ul.nav.navbar-nav
          li
            a(href='#/todo')
              i.icon.icon-list
              |  To do list
        ul.nav.navbar-nav.navbar-right
          li
            a(href='https://github.com/davinov/kewl')
              i.icon.icon-github
              |  Source code on GitHub

    .loading-indicator(ng-show='$root.loading')

    main.container(data-ng-view)

    :coffeescript
      window.onload = -> console.log Object.keys(window)
    script(src='js/vendor.js')
    script(src='js/app.js')
